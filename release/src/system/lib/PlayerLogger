sh.require("Logger")
sh.require("Filesystem")

local L = Logger.newChannel("PlayerLog")

function run()
  prox = peripheral.wrap("top")
  
  if(prox == nil) then
    L:err('No Player Detector found on top')
  else
    while prox.getPlayers() ~= "{}" do
      players = prox.getPlayers()
      for i, v in ipairs(players) do
        L:err('Player' .. v.name())
      end
    end
  end
end


