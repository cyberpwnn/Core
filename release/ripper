local dir = {}
local bse = "/release/src/"
local mse = "https://raw.githubusercontent.com/danielmills/Core/master/release/src/"

function rip()
  print("Finding all directories...")
  findin(bse)
end

function findin(dirx)
  for i,v in ipairs(fs.list(dirx))
    if fs.isDir(v) then
      findin(dirx..v)
    else
      print(dirx..v)
    end
    os.sleep(0.1)
  end
end

rip()